package onlineshopproducer;

import java.util.List;

public interface ShopProducer {
	public String addItem(String itemName, double itemPrice, double itemDiscountPercentage);

	public String updateItem(String updateItemName, double updateItemPrice);

	public String removeItem(String itemName);

	public String searchItem(String itemName);

	public List<Item> itemList();

}
